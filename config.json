{
  "name": "Onewire server (1-Wire)",
  "version": "0.2.1",
  "slug": "owserver",
  "description": "onewire server to read 1-Wire devices",
  "url": "https://github.com/turbokongen/hassio-owserver",
  "startup": "system",
  "init": false,
  "arch": ["aarch64", "amd64", "armhf", "armv7"],
  "boot": "auto", 
  "host_network": true,
  "uart": true,
  "ports": {
    "8099/tcp": "8099",
    "4304/tcp": "4304"
  },
  "options": {
    "owhttpd": true,
    "device": ""
  },
  "schema": {
    "owhttpd": "bool",
    "device": "str?"
  },
  "ingress": true,
  "panel_icon": "mdi:alpha-w-box",
  "image": "ghcr.io/lrybak/owserver/{arch}"
}
